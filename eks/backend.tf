terraform {
  required_version = "~> 1.14.0"
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.49.0"
    }
  }
  backend "s3" {
    bucket = "dev-naveen-tf-bucket"
    region = "us-east-1"
    key    = "eks/terraform.tfstate"
    //dynamodb_table = "Lock-Files"
    use_lockfile = true
    encrypt      = true
  }
}

provider "aws" {
  region = var.aws-region
}